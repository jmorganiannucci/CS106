
public class Time {
	int minutes;
	int seconds;
	
	public void print(){
		System.out.print(minutes+":"+seconds);
	}
	
	public int totalTimeSec(){
		return (minutes*60)+seconds;
	}
	
	public Times(int totalSec){
		minutes = totalSec/
	}
	
	public Time(int mins, int sec){ 
		minutes = mins;
		seconds = sec;
	}
	
}
