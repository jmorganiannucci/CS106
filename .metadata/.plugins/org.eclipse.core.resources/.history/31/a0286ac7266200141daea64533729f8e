
public class Time {
	int minutes;
	int seconds;
	
	public void print(){
		if(seconds < 10)
		System.out.print(minutes+":"+seconds);
	}
	
	public int totalTimeSec(){
		return (minutes*60)+seconds;
	}
	
	public Time(int totalSec){
		minutes = totalSec/60;
		seconds = totalSec%60;
	}
	
	public Time(int mins, int sec){ 
		minutes = mins;
		seconds = sec;
	}
	
}
