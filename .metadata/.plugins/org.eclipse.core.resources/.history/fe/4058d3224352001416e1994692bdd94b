
public class BankAccount { //made a new object to be called in finance class, to keep track of balances 
	//variables 
	double checking;
	double savings; 
	double cc; 
	
	//constructor (runs when you first create a bank account, sets everything to zero)
	public BankAccount(){ 
		checking= 0.0;
		savings= 0.0;
		cc= 0.0; 
	}

}
